{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "easycorp/easyadmin-bundle": "^4.10",
        "fakerphp/faker": "^1.23",
        "knplabs/knp-paginator-bundle": "^6.4",
        "kriswallsmith/buzz": "^1.3",
        "nelmio/api-doc-bundle": "^5.6",
        "nelmio/cors-bundle": "^2.5",
        "nesbot/carbon": "^3.6",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.26",
        "pokemon-tcg/pokemon-tcg-sdk-php": "^2.1",
        "symfony/cache": "7.*",
        "symfony/console": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/flex": "^2",
        "symfony/form": "7.*",
        "symfony/framework-bundle": "7.*",
        "symfony/http-client": "7.*",
        "symfony/http-client-contracts": "*",
        "symfony/lock": "7.*",
        "symfony/mailer": "7.*",
        "symfony/messenger": "7.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "7.*",
        "symfony/property-info": "7.*",
        "symfony/rate-limiter": "^7.3",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/serializer": "7.*",
        "symfony/validator": "7.*",
        "symfony/yaml": "7.*",
        "symfonycasts/reset-password-bundle": "^1.21",
        "tcgdex/sdk": "^2.2",
        "zircote/swagger-php": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "vendor/bin/phpstan analyse src --level=max --memory-limit=1G",
        "phpstan:baseline": "vendor/bin/phpstan analyse src --level=max --generate-baseline",
        "cs-fix": "vendor/bin/php-cs-fixer fix src",
        "cs-check": "vendor/bin/php-cs-fixer fix src --dry-run --diff",
        "quality": [
            "@phpstan",
            "@cs-check"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.64",
        "liip/test-fixtures-bundle": "^3.1",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/debug-bundle": "7.*",
        "symfony/maker-bundle": "^1.56",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "7.*",
        "symfony/web-profiler-bundle": "7.*"
    }
}
