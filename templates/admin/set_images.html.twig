{% if field.value %}
    <div class="set-images-preview">
        {% if field.value.symbol %}
            <div class="set-image-item">
                <strong>Symbole:</strong><br>
                <img src="{{ field.value.symbol }}" alt="Set symbol" style="max-width: 50px; max-height: 50px; border: 1px solid #ddd;" />
            </div>
        {% endif %}
        {% if field.value.logo %}
            <div class="set-image-item">
                <strong>Logo:</strong><br>
                <img src="{{ field.value.logo }}" alt="Set logo" style="max-width: 100px; max-height: 50px; border: 1px solid #ddd;" />
            </div>
        {% endif %}
    </div>
{% else %}
    <small style="color: #999;">Aucune image</small>
{% endif %}